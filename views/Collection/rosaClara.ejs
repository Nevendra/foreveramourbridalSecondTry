</head>
<body ng-app="CollectionImageGallery">
	<div class="additionalNav">
		<i class=" closeAdditionalNav fa fa-bars" aria-hidden="true"></i>
		<ul>

			<li><a href="additionalDesigners">Additional Designer</a></li>
			<li><a href="eddiek">Eddie K</a></li>
			<li><a href="judeJowlison">Jude Jowilson</a></li>
			<li><a href="justinAlexander">Justin Alexander</a></li>
			<li><a href="nicolespose">Nicole Spose</a></li>
			<li><a href="rosaClara">Rosa Clara</a></li>
			<li><a href="venus">Venus</a></li>

		</ul>
	</div>
	<nav>
	<div class="logoNav">
		<p><a href="/">Forever Amour Bridal Boutique</a><p>
		</div>

	<ul>
		  	<li><a href="/" class="btn btn-4">Home</a></li>
	     	<li><a href="/collection" class="btn btn-4">Collection</a></li>
		 	<li><a href="/custom" class="btn btn-4">Custom Made</a></li>
		 	<li><a href="/contact" class="btn btn-4">Contact</a></li>
		   	<li><a href="/about" class="btn btn-4">About</a></li>
		   	<li><a href="" class="btn btn-4">Start Here</a></li>

	 </ul>

		<div class="appointment">
	  		<a href="appointment">Make An Appointment</a>
		</div>

	   <!--  <div class="clear">
	    </div> -->

	</nav>

	<div ng-controller="CollectionImageGalleryController as ctrl">
		<div class="mainContent">
			<div class="additionalDesigner">
				<p>Additional Designers<span>               </span><span><i class="fa fa-bars" aria-hidden="true"></i></span></p>
			</div>
			<div class="designerAbout">
				<div ng-repeat=" designer in ctrl.rosaClara[0]">
					<h1 ng-bind="designer.name"></h1>
					<p ng-bind="designer.description"></p>
					<img src="{{designer.mainImage}}">
				</div>
			</div>
			<div class="designerGallery">
				<div class="galleryBox float" ng-repeat=" gallery in ctrl.rosaClara[1]">
					<img class="clickedImage" src="{{gallery.img}}" ng-click="ctrl.viewLargeImage(gallery.img, ctrl.rosaClara[1], $index)">
					<p ng-bind="gallery.designerName"></p>
				</div>
			</div>
			<div class="clear"></div>
		</div>

		<div class="selectedImage" ng-if="ctrl.clickedThumbNail == true">
			<div class="selectedImageContent">
				<p class="previous" ng-click="ctrl.prevPage(ctrl.selectedArray, ctrl.selectedIndex)">Previous</p>
				<img class-"theSelectedImage" src="{{ctrl.selectedImage}}">
				<p class="next" ng-click="ctrl.nextPage(ctrl.selectedArray, ctrl.selectedIndex)">Next</p>
				<P class="close" ng-click="ctrl.closeDesignerImageGallerySelectedImage()">X</P>
			</div>
			<div class="clear"></div>
		</div>
	</div>

</body>
</html>
