	<div>
		<div class="mainContent">
			<div class="additionalDesigner" ng-if="ctrl.mainHtml == true">
				<p>Additional Designers<span>               </span><span><i class="fa fa-bars" aria-hidden="true"></i></span></p>
			</div>
			<div class="designerAbout" ng-if="ctrl.mainHtml == true">
				<div class="coral" ng-repeat=" designer in ctrl.judeJowilson[0]">
					<h1 ng-bind="designer.name"></h1>
					<p ng-bind="designer.description"></p>
					<img src="{{designer.mainImage}}">
				</div>
			</div>
			<div class="designerGallery" ng-if="ctrl.mainHtml == true">
				<div class="galleryBox float" ng-repeat=" gallery in ctrl.judeJowilsonPaginate">
					<img class="clickedImage" src="{{gallery.img}}" ng-click="ctrl.viewLargeImage(gallery.img, ctrl.judeJowilsonPaginate, $index)">
					<p ng-bind="gallery.designerName"></p>
				</div>
			</div>
			<div class="clear"></div>
		</div>

		<p class="previousMain" ng-click="ctrl.prevPage(ctrl.judeJowilson[1], ctrl.current_page)">&laquo;</p>
			
		<p class="nextMain" ng-click="ctrl.nextPage(ctrl.judeJowilson[1], ctrl.current_page)">&raquo;</p>

		<div class="selectedImage" ng-if="ctrl.clickedThumbNail == true">
			<div class="selectedImageContent">
				<p class="previous" ng-click="ctrl.prevImage(ctrl.selectedArray, ctrl.selectedIndex)">Previous</p>
				<img class="theSelectedImage" src="{{ctrl.selectedImage}}">
				<p class="next" ng-click="ctrl.nextImage(ctrl.selectedArray, ctrl.selectedIndex)">Next</p>
				<P class="close" ng-click="ctrl.closeDesignerImageGallerySelectedImage()">X</P>
			</div>
			<div class="clear"></div>
		</div>
	</div>

